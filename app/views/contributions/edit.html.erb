<div class="contents row">
  <div class="container">
    <h3>編集する</h3>
    <%= form_with(model: @contribution, local: true) do |form| %>
      <%= form.text_field :title, placeholder: "title" %>
      <%= form.text_area :text, placeholder: "text" %>
      <%= form.submit "投稿する" %>
    <% end %>
  </div>
</div>